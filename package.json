{
  "name": "Croquis-ImagePicker",
  "version": "0.1.13",
  "description": "app for picking random image",
  "main": "dist/main.js",
  "private": "true",
  "scripts": {
    "start": "electron --disable-gpu --enable-transparent-visuals dist/main.js",
    "build": "tsc && sass src:dist && webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:electron": "npm run build && npm run start",
    "start:browser": "npm run build"
  },
  "build": {
    "appId": "com.electron.Croquis",
    "files": [
      "dist/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vhv3y8/Croquis-ImagePicker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vhv3y8/Croquis-ImagePicker/issues"
  },
  "homepage": "https://github.com/vhv3y8/Croquis-ImagePicker#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "html-webpack-plugin": "^5.2.0",
    "nodemon": "^2.0.7",
    "sass": "^1.32.8",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "electron-log": "^4.3.2",
    "electron-updater": "^4.3.8"
  }
}
